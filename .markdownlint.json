{
  "extends": "@ivuorinen/markdownlint-config",
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "heading-style": {
    "style": "atx"
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "required-headings": false,
  "ol-prefix": false,
  "ul-style": {
    "style": "dash"
  }
}
